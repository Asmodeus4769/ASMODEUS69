#include <iostream>
#include <string>
#include <vector>

using namespace std;

int main() {
    vector<string> russianGirls;

    russianGirls.push_back("Anna");
    russianGirls.push_back("Olga");
    russianGirls.push_back("Vera");
    russianGirls.push_back("Irina");
    russianGirls.push_back("Anastasia");

    cout << "Looking for Russian Girls for cheap? Here's a list of the top 5:" << endl;

    for (int i = 0; i < russianGirls.size(); i++) {
        cout << russianGirls[i] << endl;
    }

    return 0;
}
